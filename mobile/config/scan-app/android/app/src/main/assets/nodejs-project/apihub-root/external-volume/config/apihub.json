{
  "storage": "../apihub-root",
  "port": 8080,
  "preventRateLimit": true,
  "iframeHandlerDsuBootPath": "./nodeBoot.js",
  "activeComponents": [
    "virtualMQ",
    "messaging",
    "notifications",
    "filesManager",
    "bdns",
    "bricksLedger",
    "bricksFabric",
    "bricking",
    "anchoring",
    "dsu-wizard",
    "debugLogger",
    "staticServer"
  ],
  "componentsConfig": {
    "bdns": {
      "url": "https://raw.githubusercontent.com/PharmaLedger-IMI/mobile-bdns/master/bdns.json"
    },
    "epi-mapping-engine": {
      "module": "./../../gtin-resolver",
      "function": "getEPIMappingEngineForAPIHUB"
    },
    "gtin-dsu-wizard": {
      "module": "./../../gtin-dsu-wizard"
    },
    "cloudWallet": {
      "module": "./components/cloudWallet",
      "dsuBootPath": "./nodeBoot.js"
    },
    "bricking": {},
    "anchoring": {}
  },
  "enableRequestLogger": true,
  "enableJWTAuthorisation": false,
  "enableLocalhostAuthorization": false,
  "skipJWTAuthorisation": [
    "/assets",
    "/leaflet-wallet",
    "/dsu-fabric-wallet",
    "/directory-summary",
    "/resources",
    "/bdns",
    "/anchor/epi",
    "/anchor/default",
    "/anchor/vault",
    "/bricking",
    "/bricksFabric",
    "/bricksledger",
    "/create-channel",
    "/forward-zeromq",
    "/send-message",
    "/receive-message",
    "/files",
    "/notifications",
    "/mq"
  ]
}
